Package: tpc
Type: Package
Title: Temporal PC Algorithm
Version: 1.0
Authors@R: c(
      person(given = "Janine", family = "Witte", middle = NULL,
             email = "witte@leibniz-bips.de", role = c("aut", "cph", "cre")),
